{
  "buildCommand": "bunx prisma migrate deploy && next build",
  "installCommand": "bun install",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/api/webhooks/stripe",
      "headers": [
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        }
      ]
    }
  ]
}
